<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_height="wrap_content"
    android:layout_width="match_parent"
    android:fillViewport="true"
    android:orientation="vertical">

    <LinearLayout
        android:layout_height="1500dp"
        android:layout_width="match_parent"
        android:orientation="vertical">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="1083dp"
            tools:context=".login.RegisterActivity_main">

            <EditText
                android:id="@+id/writeName"
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:singleLine="true"
                app:layout_constraintStart_toStartOf="@+id/textView3"
                app:layout_constraintTop_toBottomOf="@+id/textView3">

            </EditText>

            <EditText
                android:id="@+id/writePhone"
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:hint="000-0000-0000"
                android:singleLine="true"
                app:layout_constraintStart_toStartOf="@+id/textView8"
                app:layout_constraintTop_toBottomOf="@+id/textView8">

            </EditText>

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:text="이메일"
                android:textSize="20dp"
                app:layout_constraintStart_toStartOf="@+id/writeRePw"
                app:layout_constraintTop_toBottomOf="@+id/writeRePw" />

            <EditText
                android:id="@+id/writePw"
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:inputType="textPassword"
                android:singleLine="true"
                app:layout_constraintStart_toStartOf="@+id/textView5"
                app:layout_constraintTop_toBottomOf="@+id/textView5">

            </EditText>

            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="이름"
                android:textSize="20dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.192"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.042" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:text="비밀번호 확인"
                android:textSize="20dp"
                app:layout_constraintStart_toStartOf="@+id/writePw"
                app:layout_constraintTop_toBottomOf="@+id/writePw" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:text="비밀번호"
                android:textSize="20dp"
                app:layout_constraintStart_toStartOf="@+id/writeId"
                app:layout_constraintTop_toBottomOf="@+id/writeId" />

            <EditText
                android:id="@+id/writeId"
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:singleLine="true"
                app:layout_constraintStart_toStartOf="@+id/textView4"
                app:layout_constraintTop_toBottomOf="@+id/textView4">

            </EditText>

            <TextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:text="전화번호"
                android:textSize="20dp"
                app:layout_constraintStart_toStartOf="@+id/writeEmail"
                app:layout_constraintTop_toBottomOf="@+id/writeEmail" />

            <EditText
                android:id="@+id/writeEmail"
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:hint="example@example.com"
                android:singleLine="true"
                app:layout_constraintStart_toStartOf="@+id/textView7"
                app:layout_constraintTop_toBottomOf="@+id/textView7">

            </EditText>

            <EditText
                android:id="@+id/writeRePw"
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:inputType="textPassword"
                android:singleLine="true"
                app:layout_constraintStart_toStartOf="@+id/textView6"
                app:layout_constraintTop_toBottomOf="@+id/textView6">

            </EditText>

            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:text="아이디"
                android:textSize="20dp"
                app:layout_constraintStart_toStartOf="@+id/writeName"
                app:layout_constraintTop_toBottomOf="@+id/writeName" />

            <EditText
                android:id="@+id/writeAge"
                android:layout_width="120dp"
                android:layout_height="38dp"
                android:layout_marginStart="12dp"
                android:layout_marginLeft="12dp"
                android:singleLine="true"
                android:textSize="20dp"
                app:layout_constraintBottom_toBottomOf="@+id/textView11"
                app:layout_constraintStart_toEndOf="@+id/textView11"
                app:layout_constraintTop_toTopOf="@+id/textView11"
                app:layout_constraintVertical_bias="0.363">

            </EditText>

            <TextView
                android:id="@+id/textView11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="나이"
                android:textSize="20dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.168"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.644">

            </TextView>

            <Spinner
                android:id="@+id/selectJob"
                android:layout_width="150dp"
                android:layout_height="25dp"
                android:layout_marginStart="12dp"
                android:layout_marginLeft="12dp"
                app:layout_constraintBottom_toBottomOf="@+id/textView9"
                app:layout_constraintStart_toEndOf="@+id/textView9"
                app:layout_constraintTop_toTopOf="@+id/textView9"
                app:layout_constraintVertical_bias="0.0">

            </Spinner>

            <TextView
                android:id="@+id/textView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:text="직업"
                android:textSize="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.168"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView11">

            </TextView>

            <TextView
                android:id="@+id/textView12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="12dp"
                android:layout_marginLeft="12dp"
                android:text="세"
                android:textSize="20dp"
                app:layout_constraintBottom_toBottomOf="@+id/writeAge"
                app:layout_constraintStart_toEndOf="@+id/writeAge"
                app:layout_constraintTop_toTopOf="@+id/writeAge"
                app:layout_constraintVertical_bias="1.0">

            </TextView>

            <TextView
                android:id="@+id/textView14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="관심사 선택"
                android:textSize="20dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.214"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.729">

            </TextView>


            <RadioGroup
                android:id="@+id/radioGroup"
                android:layout_width="150dp"
                android:layout_height="254dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView14">

                <RadioButton
                    android:id="@+id/policy_Employment"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="5dp"
                    android:text="취업"
                    android:textSize="20dp"
                    tools:layout_editor_absoluteX="126dp"
                    tools:layout_editor_absoluteY="756dp">

                </RadioButton>

                <RadioButton
                    android:id="@+id/policy_HousingSupport"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="5dp"
                    android:text="주거지원"
                    android:textSize="20dp"
                    tools:layout_editor_absoluteX="126dp"
                    tools:layout_editor_absoluteY="788dp">

                </RadioButton>

                <RadioButton
                    android:id="@+id/policy_PolicyParticipation"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="5dp"
                    android:text="정책참여"
                    android:textSize="20dp"
                    tools:layout_editor_absoluteX="126dp"
                    tools:layout_editor_absoluteY="820dp">

                </RadioButton>

                <RadioButton
                    android:id="@+id/policy_Corona"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="5dp"
                    android:text="코로나"
                    android:textSize="20dp"
                    tools:layout_editor_absoluteX="126dp"
                    tools:layout_editor_absoluteY="852dp" />

                <RadioButton
                    android:id="@+id/policy_StartUp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="5dp"
                    android:text="창업"
                    android:textSize="20dp"
                    tools:layout_editor_absoluteX="126dp"
                    tools:layout_editor_absoluteY="884dp">

                </RadioButton>

                <RadioButton
                    android:id="@+id/policy_LivingWelfare"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="5dp"
                    android:text="생활복지"
                    android:textSize="20dp"
                    tools:layout_editor_absoluteX="126dp"
                    tools:layout_editor_absoluteY="916dp">

                </RadioButton>

            </RadioGroup>

            <TextView
                android:id="@+id/textView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="지역"
                android:textSize="20dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.168"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.595" />

            <Spinner
                android:id="@+id/selectArea"
                android:layout_width="150dp"
                android:layout_height="25dp"
                android:layout_marginStart="9dp"
                app:layout_constraintBottom_toBottomOf="@+id/textView10"
                app:layout_constraintStart_toEndOf="@+id/textView10"
                app:layout_constraintTop_toTopOf="@+id/textView10">

            </Spinner>


        </androidx.constraintlayout.widget.ConstraintLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <View
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_weight=".1"></View>

            <Button
                android:id="@+id/cancel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="취소" />

            <View
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_weight="1"></View>

            <Button
                android:id="@+id/registerComplete"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="회원가입" />

            <View
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_weight=".1"></View>

        </LinearLayout>

    </LinearLayout>


</ScrollView>
